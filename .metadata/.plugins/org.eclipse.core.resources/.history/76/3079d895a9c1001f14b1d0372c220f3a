package de.uni_koeln.hs

class Location {
	
	
	String city
	String country
	float latitude 
	float longitude 
	
	
	static belongsTo = [person:Person]
	
	static constraints = {
		startLocation(nullable: true, validator:{val, obj ->
			return DateValidator.validate(val, true)
		})
		endLocation(nullable: true, validator:{val, obj ->
			return DateValidator.validate(val, true)
		})
	}
}
