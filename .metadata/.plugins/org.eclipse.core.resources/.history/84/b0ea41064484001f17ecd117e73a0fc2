package de.uni_koeln.hki.thaller

import validator.DateValidator;

class Confession {
	
	
	String title
	
	String start
	String end
	
	static belongsTo = [person:Person]
	
	static constraints = {
		start(nullable: true, validator:{val, obj ->
			return DateValidator.validate(val, true)
		})
		end(nullable: true, validator:{val, obj ->
			return DateValidator.validate(val, true)
		})
	}
}
